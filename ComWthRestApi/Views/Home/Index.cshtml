@model PokeData?

@{
    ViewData["Title"] = "Home Page";

    String name = Model != null ? Model.name : "undefined";

    if (Model.id == 591)
    {
        Model.sprites.front_default = "https://play-lh.googleusercontent.com/8ddL1kuoNUB5vUvgDVjYY3_6HwQcrg1K2fd_R8soD-e2QYj8fT9cfhfh3G0hnSruLKec=s48-rw";
    }

}

<div class="text-center">
    <h1 class="display-4">@name</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <img src="@Model.sprites.front_default" />
    @foreach (var move in Model.moves)
    {
        <li>@move.move.name</li>
        <li>@move.move.url</li>
    }
</div>
